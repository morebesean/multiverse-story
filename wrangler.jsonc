{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "multiverse-story",
    // Workers の無料プランでは 10ms が制限されるため、有料プランが推奨
    "main": ".open-next/worker.js",
    "compatibility_date": "2024-09-23",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    // 환경변수 설정 (Cloudflare Dashboard에서 Secret으로 설정 권장)
    "vars": {
        // OPENAI_API_KEY는 Cloudflare Dashboard에서 Secret으로 설정하세요
    }
}